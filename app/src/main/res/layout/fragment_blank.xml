<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:background="@drawable/cuatro"
        tools:context=".BlankFragment">


    <TextView
            android:id="@+id/tvbienvenido"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bienvenido"
            android:textSize="30sp"
            android:textColor="@color/design_default_color_primary_dark" android:layout_marginTop="280dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:fontFamily="@font/roboto_bold"/>
    <TextView
            android:id="@+id/tvnombreMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nombres"
            android:textSize="30sp"
            android:textColor="@color/design_default_color_primary_dark" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="68dp"
            app:layout_constraintTop_toTopOf="@+id/tvtipoMenu" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.504"
            android:fontFamily="@font/roboto_bold"/>
    <TextView
            android:id="@+id/tvtipoMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hello_blank_fragment"
            android:textSize="30sp"
            android:textColor="@color/design_default_color_primary_dark"
            android:fontFamily="@font/roboto_bold"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tvbienvenido"/>

</android.support.constraint.ConstraintLayout>